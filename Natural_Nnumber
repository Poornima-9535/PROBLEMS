import java.util.*;
class naturalnumber
{
    static void natural(int n)
    {
        for(int i=1;i<=n;i++)
        System.out.println(i);
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int n;
        n=sc.nextInt();
        if(n<=0)
        {
            System.out.println("Enter the positive natural number");
        }
        else
        {
            natural(n);
        }

    }
}
